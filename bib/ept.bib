	journal = {Int J Clin Pediatr Dent},
	journal-full = {International journal of clinical pediatric dentistry},
	keywords = {Electric pulp tester (EPT); Pulse oximeter; True negative (TN); True positive (TP); Vitality.},
	number = {2},
