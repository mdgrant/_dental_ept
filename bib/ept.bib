	month = {Apr},
	number = {4},
	pages = {411-4},
	pmid = {17368329},
	pst = {ppublish},
	title = {Evaluation of efficacy of a new custom-made pulse oximeter dental probe in comparison with the electrical and thermal tests for assessing pulp vitality},
