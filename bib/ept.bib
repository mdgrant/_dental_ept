	mesh = {Adolescent; Adult; Aged; Cross-Sectional Studies; Dental Pulp; Dental Pulp Test; Female; Humans; Male; Middle Aged; Predictive Value of Tests; Reproducibility of Results; Young Adult},
	number = {4},
