	date-modified = {2024-10-24 11:16:50 -0500},
	doi = {https://doi.org/10.1016/j.heliyon.2023.e14621},
	isbn = {2405-8440},
	journal = {Heliyon},
	keywords = {Tooth movement techniques Orthodontic tooth movement Dental pulp Dental pulp test},
	number = {4},
	pages = {e14621},
	st = {The effect of orthodontic tooth movement on the sensitivity of dental pulp: A systematic review and meta-analysis},
	title = {The effect of orthodontic tooth movement on the sensitivity of dental pulp: A systematic review and meta-analysis},
