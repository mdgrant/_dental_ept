	date-modified = {2024-10-24 11:16:50 -0500},
	doi = {10.1186/s12984-015-0061-1},
	dp = {NLM},
	edition = {20150812},
	isbn = {1743-0003},
	journal = {J Neuroeng Rehabil},
	keywords = {Animals Electric Stimulation *Electromagnetic Fields Humans Membrane Potentials Models, Neurological Nerve Tissue/*physiology Neurons/*physiology},
	la = {eng},
	pages = {65},
	st = {Neuron matters: electric activation of neuronal tissue is dependent on the interaction between the neuron and the electric field},
	title = {Neuron matters: electric activation of neuronal tissue is dependent on the interaction between the neuron and the electric field},
